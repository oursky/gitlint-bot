{
  "name": "@oursky/gitlint",
  "version": "0.1.0",
  "description": "gitlint-bot linter module",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run clean && npm run compile",
